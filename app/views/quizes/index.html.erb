<div id='quizes_index'>
  
<h1>Listing quizes</h1>

<table>
  <tr>
    <th>Question</th>
    <th>Answer</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @quizes.each do |quize| %>
  <tr>
    <td><%= quize.question %></td>
    <td><%= quize.answer %></td>
    <td><%= link_to 'Show', quize %></td>
    <td><%= link_to 'Edit', edit_quize_path(quize) %></td>
    <td><%= link_to 'Destroy', quize, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<p>
   <%= render 'quizes/form' %>

</p>




<%= link_to 'New Quiz', new_quize_path( :compare_id => @compare.id) %>

</div>
